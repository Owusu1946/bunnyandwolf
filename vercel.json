{
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "env": {
    "VITE_API_URL": "https://sinosply-backend.onrender.com/api/v1",
    "VITE_BACKEND_URL": "https://sinosply-backend.onrender.com"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "domains": [
    "www.sinosply.com",
    "sinosply.com",
    "bunnyandwolf.vercel.app"
  ]
} 